page {
  height: 100%;
}

.map-view {
  width: 100%;
  height: 100%;
}

map {
  width: 100%;
  height: 90%;
  position: fixed;
  background-color: #f7f7f7;
}

.bottom-view {
  width: 100%;
  position: absolute;
  top: 250rpx;
  z-index: 999;
}

.cu-card-my {
  width: 100%;
  height: 100px !important;

}

.cu-btn {
  background-color: #61f5ee !important;
  float: right;
}

.cover-bottom {
  width: 250rpx;
  height: 250rpx;
  position: absolute;
  top: calc(90% - 130rpx);
  left: calc(50% - 125rpx);
  border-radius: 10%;
}
.cov-right{
  float: right;
  padding-top: 15rpx;
  padding-right: 20rpx;
}
.cov-email{
  width: 50px;
}
.cov-persion{
  width: 50px;
  
}
.textarea-send{
  height:160rpx; 
  overflow:hide; 
  font-size:1.2rem; 
  color:#666;
  border-radius: 15rpx;
  border:1px solid#b5b5b5;
  margin:10rpx;
}
.nav-item-box{
  width: 100%;
  height: 50rpx;
  padding-bottom: 20rpx;
  background-color: rgb(255, 255, 255,0.6);
}
.location-pic{
  width: 40rpx;
  height: 40rpx;
  padding-left: 10rpx;
}
.map-bar{
  width: 100%;
  position: fixed;
  top: 150rpx;
  background-color: rgb(255, 255, 255,0.6);
}
.location{
  font-size: 15px;
  padding-top: 3rpx;
}
.item-box{
  width: 100%;
  height: 50rpx;
  padding-top: 20rpx;
}
.nav-item{
  margin-left: 20rpx;
}
.index-custom{
  height: 20rpx!important;
}
.index-input{
  width: 100%;
  height: 100rpx;
  border-bottom: solid 1px #666;
}